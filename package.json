{
    "name": "react-miniui",
    "version": "1.0.0",
    "description": "A mini react UI library",
    "main": "index.js",
    "scripts": {
        "dev": "webpack-dev-server",
        "lint": "npx eslint src/**/*.js src/**/*.jsx",
        "build": "webpack --mode=production",
        "manual:prepare": "gitbook install manual",
        "manual:build": "npm run manual:prepare && gitbook build manual public/manual",
        "manual:watch": "npm run manual:prepare && gitbook serve manual",
        "manual:pdf": "gitbook pdf"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alvinhtml/react-miniui.git"
    },
    "author": "alvinhtml@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.3.3",
        "@babel/plugin-proposal-class-properties": "^7.3.3",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.4.7",
        "babel-loader": "^8.0.5",
        "babel-plugin-import": "^1.11.0",
        "css-loader": "^2.1.0",
        "eslint": "^5.15.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "filemanager-webpack-plugin": "^2.0.5",
        "gitbook-cli": "^2.3.2",
        "html-minifier": "^3.5.21",
